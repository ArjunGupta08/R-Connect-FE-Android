<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dashboard.addPropertyFrags.AddPropertyFragment_2">

    <TextView
        android:id="@+id/aT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address"
        android:fontFamily="@font/roboto_bold"
        android:textSize="@dimen/H4"
        android:layout_marginTop="20dp"
        android:layout_marginStart="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <LinearLayout
        android:id="@+id/ll1"
        android:layout_width="500dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingHorizontal="2dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="40dp"
        app:layout_constraintTop_toBottomOf="@id/aT"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
            android:hint="Address line 1"
            android:layout_marginTop="10dp"
            android:textColorHint="@color/black"
            app:boxStrokeColor="@color/black"
            app:hintTextColor="@color/secondary"
            app:boxStrokeWidthFocused="2dp"
            app:boxCornerRadiusTopStart="10dp"
            app:boxCornerRadiusBottomStart="10dp"
            app:boxCornerRadiusBottomEnd="10dp"
            app:boxCornerRadiusTopEnd="10dp"
            app:endIconMode="clear_text"
            app:endIconTint="@color/black"
            app:layout_constraintStart_toStartOf="@id/propertyTypeLayout"
            app:layout_constraintTop_toBottomOf="@id/propertyTypeLayout"
            app:layout_constraintEnd_toEndOf="@id/propertyTypeLayout">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/roboto"
                android:inputType="text"
                android:textColorHint="@color/secondary"
                android:textColor="@color/secondary" />

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
            android:hint="Address line 2"
            android:layout_marginTop="10dp"
            android:textColorHint="@color/black"
            app:boxStrokeColor="@color/black"
            app:hintTextColor="@color/secondary"
            app:boxStrokeWidthFocused="2dp"
            app:boxCornerRadiusTopStart="10dp"
            app:boxCornerRadiusBottomStart="10dp"
            app:boxCornerRadiusBottomEnd="10dp"
            app:boxCornerRadiusTopEnd="10dp"
            app:endIconMode="clear_text"
            app:endIconTint="@color/black"
            app:layout_constraintStart_toStartOf="@id/propertyTypeLayout"
            app:layout_constraintTop_toBottomOf="@id/propertyTypeLayout"
            app:layout_constraintEnd_toEndOf="@id/propertyTypeLayout">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/roboto"
                android:inputType="text"
                android:textColorHint="@color/secondary"
                android:textColor="@color/secondary" />

        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>

<com.google.android.material.textfield.TextInputLayout
android:id="@+id/state_layout"
android:layout_width="0dp"
android:layout_height="wrap_content"
style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
android:hint="State*"
android:textColorHint="@color/black"
app:boxStrokeColor="@color/black"
app:hintTextColor="@color/secondary"
android:layout_marginTop="10dp"
app:endIconDrawable="@drawable/svg_arrow_down"
app:boxStrokeWidthFocused="2dp"
app:boxCornerRadiusTopStart="10dp"
app:boxCornerRadiusBottomStart="10dp"
app:boxCornerRadiusBottomEnd="10dp"
app:boxCornerRadiusTopEnd="10dp"
app:endIconMode="custom"
app:endIconTint="@color/black"
app:layout_constraintEnd_toStartOf="@id/city_layout"
app:layout_constraintStart_toStartOf="@id/ll1"
app:layout_constraintTop_toBottomOf="@+id/ll1">

<com.google.android.material.textfield.TextInputEditText
    android:id="@+id/state_text"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fontFamily="@font/roboto"
    android:inputType="text"
    android:textColorHint="@color/secondary"
    android:textColor="@color/secondary" />

</com.google.android.material.textfield.TextInputLayout>

<com.google.android.material.textfield.TextInputLayout
android:id="@+id/city_layout"
android:layout_width="0dp"
android:layout_height="wrap_content"
style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
android:hint="City*"
android:layout_marginStart="10dp"
android:textColorHint="@color/black"
app:boxStrokeColor="@color/black"
app:hintTextColor="@color/secondary"
android:layout_marginTop="10dp"
app:boxStrokeWidthFocused="2dp"
app:boxCornerRadiusTopStart="10dp"
app:boxCornerRadiusBottomStart="10dp"
app:boxCornerRadiusBottomEnd="10dp"
app:boxCornerRadiusTopEnd="10dp"
app:endIconMode="custom"
app:endIconDrawable="@drawable/svg_arrow_down"
app:endIconTint="@color/black"
app:layout_constraintEnd_toEndOf="@id/ll1"
app:layout_constraintStart_toEndOf="@id/state_layout"
app:layout_constraintTop_toBottomOf="@+id/ll1">

<com.google.android.material.textfield.TextInputEditText
    android:id="@+id/city_text"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fontFamily="@font/roboto"
    android:inputType="text"
    android:textColorHint="@color/secondary"
    android:textColor="@color/secondary" />

</com.google.android.material.textfield.TextInputLayout>

<com.google.android.material.textfield.TextInputLayout
android:id="@+id/pincode_layout"
android:layout_width="0dp"
android:layout_height="wrap_content"
style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
android:hint="Zip Code*"
android:textColorHint="@color/black"
app:boxStrokeColor="@color/black"
app:hintTextColor="@color/secondary"
android:layout_marginTop="10dp"
app:boxStrokeWidthFocused="2dp"
app:boxCornerRadiusTopStart="10dp"
app:boxCornerRadiusBottomStart="10dp"
app:boxCornerRadiusBottomEnd="10dp"
app:boxCornerRadiusTopEnd="10dp"
app:endIconMode="custom"
app:endIconTint="@color/black"
app:layout_constraintEnd_toStartOf="@id/country_layout"
app:layout_constraintStart_toStartOf="@id/state_layout"
app:layout_constraintTop_toBottomOf="@+id/state_layout">

<com.google.android.material.textfield.TextInputEditText
    android:id="@+id/pincode_text"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fontFamily="@font/roboto"
    android:inputType="number"
    android:textColorHint="@color/secondary"
    android:textColor="@color/secondary" />

</com.google.android.material.textfield.TextInputLayout>

<com.google.android.material.textfield.TextInputLayout
android:id="@+id/country_layout"
android:layout_width="0dp"
android:layout_height="wrap_content"
style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
android:hint="Country"
android:layout_marginStart="10dp"
android:textColorHint="@color/black"
app:boxStrokeColor="@color/black"
app:hintTextColor="@color/secondary"
android:layout_marginTop="10dp"
app:boxStrokeWidthFocused="2dp"
app:boxCornerRadiusTopStart="10dp"
app:boxCornerRadiusBottomStart="10dp"
app:boxCornerRadiusBottomEnd="10dp"
app:boxCornerRadiusTopEnd="10dp"
app:endIconMode="custom"
app:endIconDrawable="@drawable/svg_arrow_down"
app:endIconTint="@color/black"
app:layout_constraintEnd_toEndOf="@id/city_layout"
app:layout_constraintStart_toEndOf="@id/pincode_layout"
app:layout_constraintTop_toBottomOf="@+id/city_layout">

<com.google.android.material.textfield.TextInputEditText
    android:id="@+id/currency_text"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fontFamily="@font/roboto"
    android:inputType="text"
    android:textColorHint="@color/secondary"
    android:textColor="@color/secondary" />

</com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/cT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contacts"
        android:fontFamily="@font/roboto_bold"
        android:textSize="@dimen/H4"
        android:layout_marginTop="20dp"
        android:layout_marginStart="30dp"
        app:layout_constraintTop_toBottomOf="@id/pincode_layout"
        app:layout_constraintStart_toStartOf="parent"/>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/phoneLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:hint="Phone"
        android:textColorHint="@color/black"
        app:boxStrokeColor="@color/black"
        app:hintTextColor="@color/secondary"
        android:layout_marginTop="10dp"
        app:endIconDrawable="@drawable/svg_arrow_down"
        app:boxStrokeWidthFocused="2dp"
        app:boxCornerRadiusTopStart="10dp"
        app:boxCornerRadiusBottomStart="10dp"
        app:boxCornerRadiusBottomEnd="10dp"
        app:boxCornerRadiusTopEnd="10dp"
        app:endIconMode="custom"
        app:endIconTint="@color/black"
        app:layout_constraintEnd_toStartOf="@id/reservationPhoneLayout"
        app:layout_constraintStart_toStartOf="@id/ll1"
        app:layout_constraintTop_toBottomOf="@id/cT">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/phoneText"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/roboto"
            android:inputType="text"
            android:textColorHint="@color/secondary"
            android:textColor="@color/secondary" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/reservationPhoneLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:hint="Reservation Phone"
        android:layout_marginStart="10dp"
        android:textColorHint="@color/black"
        app:boxStrokeColor="@color/black"
        app:hintTextColor="@color/secondary"
        android:layout_marginTop="10dp"
        app:boxStrokeWidthFocused="2dp"
        app:boxCornerRadiusTopStart="10dp"
        app:boxCornerRadiusBottomStart="10dp"
        app:boxCornerRadiusBottomEnd="10dp"
        app:boxCornerRadiusTopEnd="10dp"
        app:endIconMode="custom"
        app:endIconDrawable="@drawable/svg_arrow_down"
        app:endIconTint="@color/black"
        app:layout_constraintEnd_toEndOf="@id/ll1"
        app:layout_constraintStart_toEndOf="@id/phoneLayout"
        app:layout_constraintTop_toBottomOf="@id/cT">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/reservationPhoneText"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/roboto"
            android:inputType="text"
            android:textColorHint="@color/secondary"
            android:textColor="@color/secondary" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/emailLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:hint="Email"
        android:textColorHint="@color/black"
        app:boxStrokeColor="@color/black"
        app:hintTextColor="@color/secondary"
        android:layout_marginTop="10dp"
        app:boxStrokeWidthFocused="2dp"
        app:boxCornerRadiusTopStart="10dp"
        app:boxCornerRadiusBottomStart="10dp"
        app:boxCornerRadiusBottomEnd="10dp"
        app:boxCornerRadiusTopEnd="10dp"
        app:endIconMode="custom"
        app:endIconTint="@color/black"
        app:layout_constraintEnd_toStartOf="@id/websiteLayout"
        app:layout_constraintStart_toStartOf="@id/state_layout"
        app:layout_constraintTop_toBottomOf="@+id/phoneLayout">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/emailText"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/roboto"
            android:inputType="text"
            android:textColorHint="@color/secondary"
            android:textColor="@color/secondary" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/websiteLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:hint="Website"
        android:layout_marginStart="10dp"
        android:textColorHint="@color/black"
        app:boxStrokeColor="@color/black"
        app:hintTextColor="@color/secondary"
        android:layout_marginTop="10dp"
        app:boxStrokeWidthFocused="2dp"
        app:boxCornerRadiusTopStart="10dp"
        app:boxCornerRadiusBottomStart="10dp"
        app:boxCornerRadiusBottomEnd="10dp"
        app:boxCornerRadiusTopEnd="10dp"
        app:endIconMode="custom"
        app:endIconDrawable="@drawable/svg_arrow_down"
        app:endIconTint="@color/black"
        app:layout_constraintEnd_toEndOf="@id/city_layout"
        app:layout_constraintStart_toEndOf="@id/emailLayout"
        app:layout_constraintTop_toBottomOf="@+id/phoneLayout">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/websiteText"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/roboto"
            android:inputType="text"
            android:textColorHint="@color/secondary"
            android:textColor="@color/secondary" />

    </com.google.android.material.textfield.TextInputLayout>



</androidx.constraintlayout.widget.ConstraintLayout>